{
  "name": "nodejs-tensorflow",
  "version": "0.0.1",
  "description": "Node.js Tensorflow binding",
  "main": "index.js",
  "engines": {
    "node": ">=6.11.0"
  },
  "scripts": {
    "install": "npm run download_libs && npm run gyp",
    "download_libs": "grunt --gruntfile tools/Gruntfile download_libtensorflow",
    "gyp": "node-gyp configure build",
    "build": "webpack --config tools/webpack.config.js",
    "test": "mocha"
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodejs-tensorflow/nodejs-tensorflow.git"
  },
  "files": [
    "package.json",
    "binding.gyp",
    "index.js",
    "tools",
    "src/**/*.cc",
    "third-party/tensorflow/tensorflow/**/*.h"
  ],
  "keywords": [
    "tensorflow"
  ],
  "author": "Guro Bokum jiojiajiu@gmail.com",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nodejs-tensorflow/nodejs-tensorflow/issues"
  },
  "homepage": "https://github.com/nodejs-tensorflow/nodejs-tensorflow#readme",
  "dependencies": {
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "node-gyp": "^3.6.2",
    "request": "^2.81.0",
    "request-progress": "^3.0.0",
    "single-line-log": "^1.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "chai": "^4.0.2",
    "mocha": "^3.4.2",
    "pbf-loader": "^1.1.0",
    "protobufjs": "^6.8.0",
    "webpack": "^1.12.9"
  }
}
